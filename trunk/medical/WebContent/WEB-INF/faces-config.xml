<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>GO_LOGIN</from-outcome>
   <to-view-id>/pages/secure/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>es</supported-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
  <resource-bundle>
   <base-name>co.com.tactusoft.medical.view.resources.resources</base-name>
   <var>msg</var>
  </resource-bundle>
  <message-bundle>co.com.tactusoft.medical.view.messages.messages</message-bundle>
 </application>
</faces-config>
